
@{
    ViewBag.Title = "Advertise";
}


<!DOCTYPE html>
<html>

<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <!-- The above 3 meta tags *must* come first in the head; any other head content must come *after* these tags -->
    <!-- Remove Tap Highlight on Windows Phone IE -->
    <meta name="msapplication-tap-highlight" content="no" />

    <title>Advertise - Through FitFlick</title>

    <!--======================================================
        CSS Stylesheets
    ======================================================-->
    <link rel='stylesheet' type='text/css' href='~/Content/vendor/bootstrap.min.css'>
    <link rel='stylesheet' type='text/css' href='~/Content/vendor/jquery.mb.YTPlayer.min.css'>
    <link rel='stylesheet' type='text/css' href='~/Content/vendor/animate.min.css'>
    <link rel='stylesheet' type='text/css' href='~/Content/vendor/ionicons.min.css'>
    <link rel='stylesheet' type='text/css' href='~/Content/vendor/linea.css'>
    <link rel='stylesheet' type='text/css' href='~/Content/vendor/waves.min.css'>
    <link rel='stylesheet' type='text/css' href='~/Content/vendor/owl.carousel.css'>
    <link rel='stylesheet' type='text/css' href='~/Content/vendor/magnific-popup.css'>
    <link rel='stylesheet' type='text/css' href='~/Content/vendor/style.css'>
    @Scripts.Render("~/bundles/jquery")
</head>

<body>
    <!--======================================================
        Preloader
    ======================================================-->

    <div id='preloader'>
        <div class='loader'>

            <center><img src='~/Content/Images/loading.gif' alt></center>
        </div>
    </div>
    <!--======================================================
        Navbar
    ======================================================-->
    <nav class='navbar navbar-fixed-top'>
        <div class='container'>

            <div class='navbar-header'>
                <button type='button' class='navbar-toggle collapsed' data-toggle='collapse' data-target='#nav-collapse' aria-expanded='false'>
                    <span class='sr-only'>Toggle navigation</span>
                    <span class='icon-bar'></span>
                    <span class='icon-bar'></span>
                    <span class='icon-bar'></span>
                </button>


                <a href='#' class='navbar-brand'>
                    <img class='logo-light' src='~/Content/Images/fitflick.png' alt>
                    <img class='logo-dark' src='~/Content/Images/fitflick1.png' alt>
                </a>
            </div>

            <div class='collapse navbar-collapse' id='nav-collapse'>

                <ul class='nav navbar-nav navbar-right'>

                    <li class='active'>
                        <a href='#intro' data-scroll>Home</a>
                    </li>

                    <li>
                        <a href='#features-1' data-scroll>Features</a>
                    </li>

                    <li>
                        <a href='#features-2' data-scroll>More Features</a>
                    </li>

                    <li>
                        <a href='#screenshots' data-scroll>Screenshots</a>
                    </li>

                    <li>
                        <a href='#clothing' data-scroll>Clothing</a>
                    </li>

                    <li>
                        <a href='#clothing-1' data-scroll>Clothing List</a>
                    </li>

                    <li>
                        <a href='#pricing' data-scroll>Pricing</a>
                    </li>

                    <li>
                        <a href='#testimonials' data-scroll>Team</a>
                    </li>

                    <li>
                        <a href='#download' data-scroll>Download</a>
                    </li>

                    <li>
                        <a href='#contact' data-scroll>Contact</a>
                    </li>
                </ul>
            </div>
        </div>
    </nav>

    <!--======================================================
            Content Section
    ======================================================-->
   @RenderBody()

    <!--======================================================
        Footer
    ======================================================-->
    <footer>
        <div class='container'>
            <div class='row'>

                <div class='col-sm-6'>
                    <p class='wow fadeInLeft' data-wow-delay='.2s'>
                        &copy; 2017 FitFlick. All Rights Reserved. &
                        @Html.ActionLink("Policy", "Policy", null, new { target="_blank" })
                    </p>
                </div>

                <div class='col-sm-6'>

                    <!-- Footer Social Icons -->
                    <ul class='footer-social wow fadeInRight' data-wow-delay='.2s'>

                        <li>
                            <a href='https://www.facebook.com/FitFlick.ie/' target="_blank">
                                <i class='ion-social-facebook'></i>
                            </a>
                        </li>
                        <li>
                            <a href='https://www.instagram.com/fitflick_/' target="_blank">
                                <i class='ion-social-instagram'></i>
                            </a>
                        </li>
                        <li>
                            <a href='https://twitter.com/fitflick_' target="_blank">
                                <i class='ion-social-twitter'></i>
                            </a>
                        </li>
                    </ul>

                </div>
            </div>
        </div>
    </footer>

    <!--======================================================
        JavaScript Files
    ======================================================-->
    <script src='/Scripts/vendor/bootstrap.min.js'></script>
    <script src='/Scripts/vendor/jquery.magnific-popup.min.js'></script>
    <script src='/Scripts/vendor/jquery.ajaxchimp.min.js'></script>
    <script src='/Scripts/vendor/jquery.mb.YTPlayer.min.js'></script>
    <script src='/Scripts/vendor/parallax.min.js'></script>
    <script src='/Scripts/vendor/particles.min.js'></script>
    <script src='/Scripts/vendor/waves.min.js'></script>
    <script src='/Scripts/vendor/owl.carousel.min.js'></script>
    <script src='/Scripts/vendor/wow.min.js'></script>
    <script src='/Scripts/vendor/validator.min.js'></script>
    <script src='/Scripts/vendor/smooth-scroll.min.js'></script>
    <script src='/Scripts/vendor/script.js'></script>

    @*@Scripts.Render("~/bundles/jqueryval")*@
    @RenderSection("scripts", required: false)
    <script>
			/*=========================================================================
				YTPlayer Plugin Initialization
			=========================================================================*/
			$(function(){
				$(".player").YTPlayer();
			});
    </script>
</body>
</html>